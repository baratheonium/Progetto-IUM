<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ium.baratheon.progetto_ium">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/futsal_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity
            android:name=".LoginActivity"
            android:label="EasyCourt">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomepageActivity"
            android:label="Le tue prenotazioni"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CourtListActivity"
            android:label="Elenco strutture"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CourtDetailActivity"
            android:label="Dettaglio struttura" />
        <activity
            android:name=".RegistrationActivity"
            android:label="Registrazione" />
        <activity
            android:name=".CompleteRegistrationActivity"
            android:label="Conferma registrazione" />
        <activity
            android:name=".NewReservationActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ReservationSearchResultActivity"></activity>
    </application>

</manifest>